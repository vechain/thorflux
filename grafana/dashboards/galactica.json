          "query": "\nt1 = from(bucket: \"vechain\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"block_stats\")\n  |> filter(fn: (r) =>\n      r[\"_field\"] == \"priority_fee_open\" or\n      r[\"_field\"] == \"priority_fee_close\" or\n      r[\"_field\"] == \"priority_fee_high\" or\n      r[\"_field\"] == \"priority_fee_low\"\n  )\n\n\nt2 = from(bucket: \"vechain\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"block_stats\")\n  |> filter(fn: (r) => r[\"_field\"] == \"candlestick_tx_count\")\n  |> map(fn: (r) => ({\n      _time: r._time,\n      _value: float(v: r._value),\n      _field: r._field,\n      _measurement: r._measurement,\n      _start: r._start,\n      _stop: r._stop\n  }))\n\nunion(tables: [t1, t2])\n  |> group(columns: [])\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> sort(columns: [\"_time\"])\n  |> yield(name: \"candlestick\")",
          "refId": "A"
