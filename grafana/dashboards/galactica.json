            "uid": "B87265B08D314AF"
          },
          "query": "from(bucket: \"${bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) =>\n      r._measurement == \"transactions\" and\n      r._field       == \"validator_rewards\"\n    )\n  // only keep the raw fields well need\n  |> keep(columns: [\"block_number\", \"signer\", \"_value\"])\n  // 1) parse block_number → int “block”\n  // 2) scale non-zero tips by 10^13 → “scaled”\n  |> map(fn: (r) => ({\n      block:  int(v: r.block_number),\n      signer: r.signer,\n      scaled: if r._value == 0.0 then 0.0 else r._value / 10000000000000.0\n    }))\n  // 3) drop the now‐unused originals\n  |> drop(columns: [\"block_number\", \"_value\"])\n  // 4) merge all records together\n  |> group()\n  // 5) pivot so each signer is its own column, keyed by block\n  |> pivot(\n      rowKey:     [\"block\"],\n      columnKey:  [\"signer\"],\n      valueColumn:\"scaled\"\n    )\n  // 6) fill missing signers with zero\n  |> fill(value: 0.0)\n  // 7) sort by block ascending\n  |> sort(columns: [\"block\"])\n  |> yield(name: \"validator_tip_per_block\")\n",
          "refId": "A"
        }
      ],
      "title": "Tips per block",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": "_value {_start=\"[\\d\\-:. +UTC]*\", _stop=\"[\\d\\-:. +UTC]*\", signer=\"([\\dxabcdef]*)\"}",
            "renamePattern": "$1"
          }
        }
      ],
      "type": "barchart"
