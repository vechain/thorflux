        {
          "datasource": {
            "type": "influxdb",
            "uid": "B87265B08D314AF"
          },
          "query": "import \"math\"\n\n// Main series: convert block_base_fee to float, aggregate by window,\n// and scale the value by 10^13 to work in normalized units.\nmain = from(bucket: \"${bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"block_stats\")\n  |> filter(fn: (r) => r[\"_field\"] == \"block_base_fee\")\n  |> map(fn: (r) => ({ r with _value: float(v: r._value) }))\n  |> group(columns: [\"_field\"])\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> map(fn: (r) => ({ r with _value: r._value / math.pow(x: 10.0, y: 13.0) }))\n\n// Horizontal line series: reuse the main series timestamps while setting the value to 2.\n// (That is, 2 * 1e13 wei divided by 1e13 = 2.)\nhline = main\n  |> map(fn: (r) => ({ r with _value: 2.0, _field: \"Max Legacy Tx Gas Price\" }))\n\n// Union the two series so both are shown in the same panel.\nunion(tables: [main, hline])\n  |> yield(name: \"result\")\n",
          "refId": "A"
        }
      ],
      "title": "Block Base Fee",
      "type": "timeseries"
    },
